<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>EmployeeCrudOpJsp</display-name>
  <welcome-file-list>
    <welcome-file>Login.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>validate user</servlet-name>
    <servlet-class>com.demo.servlet.ValidateEmployee </servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>validate user</servlet-name>
    <url-pattern>/validate</url-pattern>
  </servlet-mapping>

<servlet>
    <servlet-name>display</servlet-name>
    <servlet-class>com.demo.servlet.EmpDisplay </servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>display</servlet-name>
    <url-pattern>/EmpDisplay</url-pattern>
  </servlet-mapping>
  
  

   
</web-app>